{
  "name": "DEBUGSERVER",
  "hostName": "localhost",
  "port": 1234,
  "debug": true,
  "logAccess": false,
  "logFormat": "dev",
  "useCors": true,
  "orgs": ["DBL","eBible","DCS","Vachan","DEBUGSERVER"],
  "processFrequency": "1 min",
  "nWorkers": 1,
  "verbose": true,
  "includeMutations": true,
  "sessionTimeoutInMins": 10,
  "staticPaths": [
    {
      "path": "./src/static",
      "url": "/static"
    },
    {
      "path": "../diegesis-admin-client/build",
      "url": "/admin"
    },
    {
      "path": "../diegesis-upload-client/build",
      "url": "/uploads",
      "redirectTarget": "../diegesis-upload-client/build/index.html",
      "redirects":[
        "/uploads/add"
      ]
    },
    {
      "path": "../diegesis-user-client/build",
      "url": "/",
      "redirectTarget": "../diegesis-user-client/build/index.html",
      "redirects": [
        "/who",
        "/how",
        "/list",
        "/blend",
        "/entry/*"
      ]
    }
  ],
  "deleteGenerated": false

}
